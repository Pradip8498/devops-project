pipeline {
	agent any 
	stages {
		stage ("httpd-install") {
			steps {
				sh "rm -rf *"
				sh "yum install httpd -y"
				sh "service httpd start"
				sh "echo 'this is 23q2 branch-from declarative pipeline, used (SCM)' >> /var/www/html/index.html"
				sh "chmod -R 777 /var/www/html/index.html"	
			}
		}
	}	
}		
