pipeline {
	agent {
		label {
			label "built-in"
		}
	}
	stages {
		stage ("httpd-install") {
			steps {
				sh "rm -rf /var/www/html/*"
				sh "sudo yum install httpd -y"
				sh "sudo service httpd start"
				sh "echo 'this is 23q1 branch-from declarative pipeline, used (SCM)' >> /var/www/html/index.html"
				sh "chmod -R 777 /var/www/html/index.html"	
			}
		}
	}	
}		
